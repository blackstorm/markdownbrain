{
  "name": "markdownbrain",
  "version": "0.1.5",
  "description": "MarkdownBrain frontend assets",
  "scripts": {
    "build:console": "tailwindcss -i ./console.css -o ./resources/publics/console/css/app.css --minify",
    "watch:console": "tailwindcss -i ./console.css -o ./resources/publics/console/css/app.css && tailwindcss -i ./console.css -o ./resources/publics/console/css/app.css --watch",
    "build:frontend": "tailwindcss -i ./frontend.css -o ./resources/publics/frontend/css/frontend.css --minify",
    "watch:frontend": "tailwindcss -i ./frontend.css -o ./resources/publics/frontend/css/frontend.css && tailwindcss -i ./frontend.css -o ./resources/publics/frontend/css/frontend.css --watch",
    "build": "npm run build:console && npm run build:frontend",
    "watch": "npm run watch:console & npm run watch:frontend & wait"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/cli": "4.1.18",
    "tailwindcss": "4.1.18"
  },
  "dependencies": {}
}
